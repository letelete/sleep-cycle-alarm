<?xml version="1.0" encoding="utf-8"?>
<com.gmail.brunokawka.poland.sleepcyclealarm.ui.tabs.accessalarm.alarms.AlarmsEditAlarmView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/alarmsEditRingtoneClickable"
        android:padding="@dimen/medium_margin_size"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/alarmsEditRingtoneTitle"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/pref_ringtone_select_title"
            android:textColor="@color/text_high_emphasis"
            android:textSize="@dimen/dialog_edit_alarm_title_text_size" />

        <TextView
            android:text="@string/pref_ringtone_select_selected_summary"
            android:textColor="@color/text_medium_emphasis"
            android:id="@+id/alarmsEditRingtoneSummary"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
    </LinearLayout>

</com.gmail.brunokawka.poland.sleepcyclealarm.ui.tabs.accessalarm.alarms.AlarmsEditAlarmView>